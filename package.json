{
  "name": "@jaebradley/wakatime-cli",
  "version": "0.0.0-development",
  "description": "CLI for the Wakatime Service",
  "publishConfig": {
    "access": "public"
  },
  "main": "./build/index.js",
  "engines": {
    "node": "^8 || ^10 || ^11"
  },
  "bin": {
    "waka": "./build/executables/waka.js"
  },
  "global": true,
  "scripts": {
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "compile": "babel -d build/ src/ --ignore node_modules,*.test.js",
    "compile:prod": "BABEL_ENV=production npm run compile",
    "lint": "eslint --ext .js .",
    "test": "jest --coverage --passWithNoTests",
    "prepublishOnly": "npm run compile:prod",
    "semantic-commit": "commit",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/wakatime-cli.git"
  },
  "keywords": [
    "wakatime",
    "waka time",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/jaebradley/wakatime-cli/issues"
  },
  "homepage": "https://github.com/jaebradley/wakatime-cli#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "bar-horizontal": "^0.4.0",
    "chalk": "^2.4.2",
    "commander": "^3.0.2",
    "inquirer": "^7.0.0",
    "keytar": "^4.13.0",
    "luxon": "^1.19.3",
    "wakatime-client": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@commitlint/prompt": "^8.2.0",
    "@commitlint/prompt-cli": "^8.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-preset-minify": "^0.5.1",
    "codecov": "^3.6.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "semantic-release": "^16.0.0"
  }
}
